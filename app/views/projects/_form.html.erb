<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :proj_name %>
    <%= f.input :user_id %>
    <%= f.input :start_date %>
    <%= f.input :date_due %>
    <%= f.input :progress %>
    <%= f.input :about %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
